
#include<stdio.h>

int main()
{
  int a,b,c,d,g,h,e,f,p,q;
  scanf("%d%d",&a,&b);
  printf("%d and %d is the coordinates of bottom-left corner of rectangle\n",a,b);
  scanf("%d%d",&c,&d);
  printf("%d and %d is the coordinates of top right corner\n",c,d);
  scanf("%d%d%d%d,&a,&b,&c,&d");
  printf("The four corners of rectangle are (a,b),(g,h),(c,d),(e,f)\n");
  scanf("%d%d",&p,&q);
  printf("check if p and q is inside or not\n");
  printf("(p,q) is inside the rectangle\n");












}
