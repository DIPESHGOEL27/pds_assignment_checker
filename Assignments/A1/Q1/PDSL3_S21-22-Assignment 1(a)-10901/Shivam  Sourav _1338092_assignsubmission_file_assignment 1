#include <stdio.h>
void main()
{
    float a,u,alpha,t;
    printf("enter the values of a,u,alpha");
    scanf("%f%f%f",&a,&u,&alpha);
    printf("enter the time");
    scanf("%f",&t);
    printf("distance=%f",a+u*t+0.5*alpha*t*t);
}