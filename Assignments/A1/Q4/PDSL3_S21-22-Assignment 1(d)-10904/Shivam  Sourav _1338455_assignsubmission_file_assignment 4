#include <stdio.h>
#include<math.h>
void main()
{
    float a1,a2,b1,b2,c1,c2,l1,l2,l3,s;
printf("enter the three cordinates of a triangle");
scanf ("%f%f%f%f%f%f",&a1,&a2,&b1,&b2,&c1,&c2);
l1=sqrt((b1-a1)*(b1-a1)+(b2-a2)*(b2-a2));
l2=sqrt((c1-b1)*(c1-b1)+(c2-b2)*(c2-b2));
l3=sqrt((c1-a1)*(c1-a1)+(c2-a2)*(c2-a2));
if(((l1+l2)>l3)&&((l2+l3)>l1)&&((l1+l3)>l2))
{
    printf("valid triangle");
    if((l1>=l2)&&(l1>=l3))
    {
        s=(l2*l2+l3*l3)-(l1*l1);
        if(s=0)
        printf("right anfled triangle");
        else if ((l2*l2+l3*l3-l1*l1)<0)
        printf("obtuse angled triangle");
        else if (l2*l2+l3*l3-l1*l1>0)
        printf("acute angled");
    }
    else if((l2>=l1)&&(l2>=l3))
    {
        s=(l1*l1+l3*l3)-(l2*l2);
        if((s=0))
        printf("right anfled triangle");
        else if ((l1*l1+l3*l3)-(l2*l2)<0)
        printf("obtuse angled triangle");
        else if ((l1*l1+l3*l3)-(l2*l2)>0)
        printf("acute angled");
    }
    else  if((l3>=l2)&&(l3>=l1))
    {
        s=(l2)*(l2)+(l1)*(l1)-(l3)*(l3);
        if(s=0)
        printf("right anfled triangle");
        else if ((l2*l2+l1*l1-l3*l3)<0)
        printf("obtuse angled triangle");
        else if (l2*l2+l1*l1-l3*l3>0)
        printf("acute angled");
    }
}
else
printf("invalid triangle");
}