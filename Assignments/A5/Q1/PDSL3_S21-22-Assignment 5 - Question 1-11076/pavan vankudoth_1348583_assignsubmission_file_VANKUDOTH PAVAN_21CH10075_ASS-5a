#include <stdio.h>

void sorter (int * p1, int * p2, int * p3)
{
    int temp;
    if (*p1>*p2){
        temp=*p1;
        *p1=*p2;
        *p2=temp;
    }
    if (*p2>*p3){
        temp=*p2;
        *p2=*p3;
        *p3=temp;
    }
}

int main()
{
    int a,b,c;
    printf("Enter three integers : ");
    scanf("%d %d %d",&a,&b,&c);
    printf("\n--- Before Function Call ---");
    printf("\nAddress of a : %d",&a);
    printf("\nAddress of b : %d",&b);
    printf("\nAddress of c : %d",&c);
    sorter(&a,&b,&c);
    printf("\n\n--- After Function Call ---");
    printf("\nAddress of a : %d",&a);
    printf("\nAddress of b : %d",&b);
    printf("\nAddress of c : %d",&c);
    printf("\n\nValues in a,b,c = %d, %d, %d",a,b,c);
    return 0;
}
